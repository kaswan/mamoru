<table class="history">
  <thead>
    <tr>
      <th><%= _('Name') %></th>
      <th><%= _('Created') %></th>      
    </tr>    
  </thead>
  <tbody>
    <% @user.supervised_users.each do |supervisor|%>
      <tr>
        <td><%= supervisor.admin_user.try(:name)%></td>
        <td><%= l supervisor.try(:created_at), format: :day_hour_minute %></td>        
      </tr>
    <% end %>
  </tbody>
</table>  