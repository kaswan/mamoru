<% if current_user %>
  <div class="page-header">
    <h1><%= current_user.name %></h1>
  </div>
  <div class="panel panel-default">
    <div class="panel-body">
      <ul>
        <li><strong><%= link_to "Posts", posts_path %></strong></li>
        <li><strong><%= link_to "User Profile", profiles_path %></strong></li>
        <li><strong><%= link_to 'Change Password', edit_user_registration_path %></strong></li>
      </ul>
    </div>
  </div>
<% else %>
  <%= render 'welcome' %>
<% end %>